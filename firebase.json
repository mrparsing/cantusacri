{
  "hosting": {
    "public": "/",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          { "key": "Cache-Control", "value": "max-age=0, no-cache, no-store, must-revalidate" }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/nav-bar/testo/**",
        "destination": "/nav-bar/testo.html"
      },
      {
        "source": "/celebrazione/**",
        "destination": "/celebrazioni.html"
      }
    ]
  }
}